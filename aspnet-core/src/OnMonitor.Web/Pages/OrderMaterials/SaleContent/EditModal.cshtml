@page
@inherits OnMonitor.Web.Pages.OnMonitorPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model OnMonitor.Web.Pages.OrderMaterials.SaleContent.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="SaleContent" data-ajaxForm="true" asp-page="EditModal">
    <abp-modal>
        <abp-modal-header title="@L["EditSaleContent"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>