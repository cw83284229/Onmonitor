@page
@inherits OnMonitor.Web.Pages.OnMonitorPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model OnMonitor.Web.Pages.MenusInfos.SystemMenu.CreateModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="SystemMenu" data-ajaxForm="true" asp-page="CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["CreateSystemMenu"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>